#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sefaarslan/lms-platform/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.6.0_@swc+core@1.13.3_chokidar@4.0.3__@swc+core@1.13.3_@types+node@22.17.1/node_modules/@nestjs/cli/bin/node_modules:/Users/sefaarslan/lms-platform/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.6.0_@swc+core@1.13.3_chokidar@4.0.3__@swc+core@1.13.3_@types+node@22.17.1/node_modules/@nestjs/cli/node_modules:/Users/sefaarslan/lms-platform/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.6.0_@swc+core@1.13.3_chokidar@4.0.3__@swc+core@1.13.3_@types+node@22.17.1/node_modules/@nestjs/node_modules:/Users/sefaarslan/lms-platform/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.6.0_@swc+core@1.13.3_chokidar@4.0.3__@swc+core@1.13.3_@types+node@22.17.1/node_modules:/Users/sefaarslan/lms-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sefaarslan/lms-platform/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.6.0_@swc+core@1.13.3_chokidar@4.0.3__@swc+core@1.13.3_@types+node@22.17.1/node_modules/@nestjs/cli/bin/node_modules:/Users/sefaarslan/lms-platform/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.6.0_@swc+core@1.13.3_chokidar@4.0.3__@swc+core@1.13.3_@types+node@22.17.1/node_modules/@nestjs/cli/node_modules:/Users/sefaarslan/lms-platform/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.6.0_@swc+core@1.13.3_chokidar@4.0.3__@swc+core@1.13.3_@types+node@22.17.1/node_modules/@nestjs/node_modules:/Users/sefaarslan/lms-platform/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.6.0_@swc+core@1.13.3_chokidar@4.0.3__@swc+core@1.13.3_@types+node@22.17.1/node_modules:/Users/sefaarslan/lms-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nestjs/cli/bin/nest.js" "$@"
else
  exec node  "$basedir/../@nestjs/cli/bin/nest.js" "$@"
fi
